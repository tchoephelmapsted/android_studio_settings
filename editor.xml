<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <option name="HIGHLIGHT_SCOPE" value="true" />
    <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="java.util.logging.Handler" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Fira Code Retina" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="SECONDARY_FONT_FAMILY" value="Fira Code Medium" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="ObjectiveC" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc;*.java, *.xml" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseLambdas" value="false" />
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="OCCodeFoldingSettings">
    <option name="collapseConditionallyNotCompiled" value="false" />
    <option name="collapseNewLineBeforeLBrace" value="false" />
    <option name="collapseTemplateParamList" value="false" />
    <option name="COLLAPSE_TEMPLATE_PARAM_LIST" value="false" />
    <option name="COLLAPSE_CONDITIONALLY_NOT_COMPILED" value="false" />
    <option name="COLLAPSE_NEW_LINE_BEFORE_LBRACE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" pattern="\bTODO:.*METADATASPLIT.*" />
    <pattern icon="important" useCustomColors="true" pattern="\b(deleteme|delete\sme)\b.*">
      <option name="FOREGROUND" value="c04195" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
  <component name="XmlFoldingSettings">
    <option name="COLLAPSE_HTML_STYLE_ATTRIBUTE" value="false" />
    <option name="COLLAPSE_ENTITIES" value="false" />
    <option name="COLLAPSE_DATA_URI" value="false" />
  </component>
</application>